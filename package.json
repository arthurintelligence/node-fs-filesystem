{
  "name": "fs-filesystem",
  "version": "2.1.2",
  "description": "Read information about the filesystem of the current machine (devices and partitions)",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "repository": "https://github.com/qzdio/node-fs-filesystem",
  "author": "philippefutureboy <philippe@human.space>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "npm run clean && npm run build:cjs && npm run build:esm",
    "build:cjs": "cross-env BABEL_ENV=commonjs babel -d lib/ src/",
    "build:esm": "cross-env BABEL_ENV=es babel -d esm/ src/",
    "clean": "rimraf lib && rimraf esm",
    "coverage": "istanbul cover -i 'lib/**' -x '**/__tests__/**' test.js",
    "coveralls": "coveralls < coverage/lcov.info",
    "lint": "npm run lint:fix",
    "lint:warn": "eslint ./src/*.js ./test/*.js",
    "lint:fix": "eslint --fix ./src/*.js ./test/*.js",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test:ci && npm run build",
    "show": "npm run build && node ./lib/devel.js",
    "release": "release-it",
    "test": "npm run lint && nyc npm run unit && nyc report --reporter=lcov",
    "test:ci": "npm run lint && nyc npm run unit && nyc report --reporter=text-lcov | coveralls",
    "test:no-cov": "npm run lint && npm run unit",
    "unit": "mocha --require babel-register",
    "unit:debug": "mocha --require babel-register --inspect-brk",
    "watch": "npm run build:cjs -- -w"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "bluebird": "^3.5.1",
    "ramda": "^0.25.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.2",
    "coveralls": "^3.0.2",
    "cross-env": "^5.2.0",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.2.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "mockery": "^2.1.0",
    "nyc": "^12.0.2",
    "release-it": "^7.4.8",
    "rewire": "^4.0.1",
    "rimraf": "^2.6.2",
    "sinon": "^6.1.4"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
